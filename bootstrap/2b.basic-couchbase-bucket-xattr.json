{
  "bootstrap": {
    "server": "couchbases://127.0.0.1",
    "server_tls_skip_verify": true,
    "username": "syncGatewayUser",
    "password": "password"
  },
  "api": {
    "public_interface": "0.0.0.0:4984",
    "admin_interface": ":4985",
    "metrics_interface": ":4986",
    "https": {}
  },
  "logging": {
    "console": {
      "rotation": {},
      "log_level": "debug",
      "log_keys": [
        "Import",
        "Sync",
        "SyncMsg",
        "CRUD",
        "Changes",
        "Cache",
	"Replicate"
      ]
    },
    "error": {
      "enabled": true,
      "rotation": {
        "max_size": 20,
        "max_age": 180
      }
    },
    "warn": {
      "enabled": true,
      "rotation": {
        "max_size": 20,
        "max_age": 90
      }
    },
    "info": {
      "enabled": true,
      "rotation": {}
    },
    "debug": {
      "enabled": true,
      "rotation": {}
    },
    "trace": {
      "rotation": {}
    },
    "stats": {
      "rotation": {}
    }
  },
  "auth": {},
  "replicator": {},
  "unsupported": {}
}
